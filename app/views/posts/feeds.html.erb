<div class="container">
    <div class="col-xs-8 col-xs-offset-2" style="padding: 0px;">
        <% if @posts.present? %>
           <%= render 'posts'%>
        <% else %>
          <p>There are no posts containing the term(s) <%= params[:search] %>.</p>
        <% end %>
        <div class="flickr_pagination">
            <%= will_paginate @posts %>
        </div>
    </div>
</div>